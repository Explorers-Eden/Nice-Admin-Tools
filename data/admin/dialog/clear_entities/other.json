{
  "type": "minecraft:confirmation",
  "title": "Nice Admin Tools: Clear Entities",
  "external_title": "Nice Admin Tools: Clear Entities",
  "inputs": [
    {
      "type": "minecraft:text",
      "key": "type",
      "label": "Entity Or Entity Tag List To Clear:",
      "initial": "minecraft:creeper"
    },
    {
      "type": "minecraft:number_range",
      "key": "distance",
      "label": "Distance",
      "label_format": "%s: %s Blocks",
      "start": 1,
      "end": 1024,
      "step": 1,
      "initial": 128
    },
    {
      "type": "minecraft:boolean",
      "key": "passengers",
      "label": "Only Entities Without Passengers",
      "on_true": "1",
      "on_false": "0"
    },
    {
      "type": "minecraft:boolean",
      "key": "leash",
      "label": "Only Entities That Are Not Leashed",
      "on_true": "1",
      "on_false": "0"
    },
    {
      "type": "minecraft:boolean",
      "key": "unnamed",
      "label": "Only Entities Without Custom Name",
      "on_true": "1",
      "on_false": "0"
    }
  ],
  "can_close_with_escape": true,
  "pause": false,
  "after_action": "close",
  "yes": {
    "label": "Confirm",
    "action": {
      "type": "minecraft:dynamic/run_command",
      "template": "function admin:clear_entities/other {unnamed:$(unnamed),type:'$(type)',passengers:$(passengers),leash:$(leash),distance:$(distance)}"
    }
  },
  "no": {
    "label": "Back",
    "action": {
      "type": "minecraft:show_dialog",
      "dialog": "admin:clear_entities/main"
    }
  }
}